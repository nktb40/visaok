	<script type="text/javascript">
	
	
	$(document).ready(function(){
	
	$(".intro").backstretch("<%= asset_path 'header_gb3.jpg' %>");
	
	$( "#new_order" ).submit(function( event ) {
		if ( $( "#order_name" ).val() ) {
				if ( $( "#order_phone" ).val()) {
					if ( $( "#order_email" ).val()) {
					   $('#order_submit').attr("disabled", true);
					   $('#order_submit').css('background-image', 'url(<%=asset_url("load_o.gif") %>)');
					   $('#order_submit').fadeIn(10);	
	  					$('#order_submit').val('Подождите');
	  					return;
	  				}
	  				$( "#email_error" ).text("Укажите Ваш e-mail");
	  				return false;
	  			}
	  		$( "#phone_error" ).text("Укажите Ваш номер телефона");
	  		return false;
	  	}
	  	$( "#name_error" ).text("Укажите Ваше имя");
	  	return false;
	});
	
	$( "#new_question" ).submit(function( event ) {
		if ( $( "#question_name" ).val() ) {
	  		if ( $( "#question_email" ).val() ) {
	  			if ( $( "#question_question" ).val() ) {
	  			   $('#question_submit').attr("disabled", true);
	  				$('#question_submit').css('background-image', 'url(<%=asset_url("load_q.gif") %>)');
					$('#question_submit').fadeIn(10);	
					$('#question_submit').val('Подождите');  				
	  				return;
	  			}
	  			//$( "#question_question" ).attr("placeholder","Напишите Ваш вопрос");
	  			//$( "#question_question" ).css("-webkit-input-placeholder","red");
	  			//$( "#question_question" ).css("-moz-placeholder","red");
	  			//$( "#question_question" ).css("-ms-input-placeholder","red");
	  			return false;
	  		}
	  		$( "#question_email" ).attr("placeholder","Укажите Ваш e-mail");
	  		return false;
	  	}
	  	$( "#question_name" ).attr("placeholder","Укажите Ваше имя");
  		return false;
	});

		$('.nav').onePageNav();	
		
		var nav = responsiveNav("#nav", { // Selector
		  animate: true, // Boolean: Use CSS3 transitions, true or false
		  transition: 400, // Integer: Speed of the transition, in milliseconds
		  label: "<i class='icon-reorder'>​</i>​", // String: Label for the navigation toggle
		  insert: "after", // String: Insert the toggle before or after the navigation
		  customToggle: "", // Selector: Specify the ID of a custom toggle
		  openPos: "relative", // String: Position of the opened nav, relative or static
		  jsClass: "js", // String: 'JS enabled' class which is added to <html> el
		  init: function(){}, // Function: Init callback
		  open: function(){}, // Function: Open callback
		  close: function(){} // Function: Close callback
		});

	});
	
	
	</script>
	
